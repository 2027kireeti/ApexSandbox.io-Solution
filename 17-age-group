public String ageGroup(Integer n) {
    //code here
    if(n>=0 and n<=1){
        return 'Infant';
    }
    else{
        if(n>=2 and n<=14){
            return 'Child';
        }
        else{
            if(n>=15 and n<=21){
                return 'Youth';
            }
            else{
                return 'Adult';
            }
        }
    }
}